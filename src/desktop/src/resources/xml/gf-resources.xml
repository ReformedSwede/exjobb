<?xml version="1.0" encoding="utf-8"?>
<resources>

    <language lang="eng">
        <category name="Noun" oper="mkN" minArgs="1">
            <inflection-form name="Sing" form="Sg Nom"/>
            <inflection-form name="Plur" form="Pl Nom"/>
        </category>
        <category name="Verb" oper="mkV" minArgs="1">
            <inflection-form name="Infinitive" form="VInf"/>
            <inflection-form name="Past" form="VPast"/>
            <inflection-form name="PPart" form="VPPart"/>
        </category>
    </language>
    <language lang="swe">
        <category name="Noun" oper="mkN" minArgs="1">
            <inflection-form name="Sing" form="Sg Indef Gen"/>
            <inflection-form name="Plur" form="Pl Indef Nom"/>
        </category>
        <category name="Verb" oper="mkV" minArgs="1">
            <inflection-form name="Infinitive" form="(VI (VInfin Act))"/>
            <inflection-form name="Past" form="(VF (VImper Act))"/>
            <inflection-form name="PPart" form="(VI (VSupin Arc))"/>
        </category>
    </language>


    <cat name="Noun" cat="NF">
        <inflection name="Sing"/>
        <inflection name="Plur"/>
    </cat>
    <cat name="Verb" cat="VF">
        <inflection name="Infinitive"/>
        <inflection name="Past"/>
        <inflection name="PPart"/>
    </cat>

</resources>
